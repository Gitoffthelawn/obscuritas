{
    "manifest_version": 3,
    "name": "Obscuritas",
    "version": "2.0",
    "description": "Firefox addon that replace all light colors to dark colors on every page",
    "icons": {
        "48": "icons/obscuritas-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "run_at": "document_start",
            "js": [
                "obscuritas.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "script.js"
    ]
}
