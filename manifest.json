{
    "manifest_version": 2,
    "name": "Obscuritas",
    "version": "1.0",
    "description": "Firefox addon that replace all light colors to dark colors on every page",
    "icons": {
        "48": "icons/obscuritas-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "obscuritas.js"
            ]
        }
    ]
}